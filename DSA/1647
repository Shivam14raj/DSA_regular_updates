class Solution {
public:
    int minDeletions(string s) {
        vector<int> freq(26,0);
        for(char c: s){
            freq[c-'a']++;
        } 
        unordered_set<int> st; 
        int del=0; 

        for(int f: freq){
            while(f>0 && st.count(f)){
                f--;
                del++;
            }
            if(f>0) st.insert(f); 
        }
       
       return del; 

    }
};